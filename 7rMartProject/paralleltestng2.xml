<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="ParallelTesting" parallel="tests" thread-count="2">
	<test name="Test1">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="testscript.LoginTest">
				<methods>
					<include
						name="verifyUserLoginWithValidUsernameAndValidPassword">
					</include>
				</methods>
			</class>
		</classes>
	</test>
	<test name="Test2">

		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="testscript.AdminUsersTest">
				<methods>
					<include
						name="verifyWhetherUserIsAbleToAddNewUserToTheUsersList">
					</include>
				</methods>
			</class>
		</classes><!--Test -->
	</test>
</suite> <!--
Suite -->
